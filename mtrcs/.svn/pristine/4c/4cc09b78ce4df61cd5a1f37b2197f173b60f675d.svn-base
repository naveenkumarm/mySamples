.shiny-button {
  display: inline-block;
  text-align: center;
  border-width: 1px;
  border-style: solid;
  text-transform: none;
  text-decoration: none;
  line-height: 1.1;
  font-weight: bold;
  color: #FFFFFF;
  font-size: 12px;
  background-color: #0083BF;
  background-image: -webkit-linear-gradient(top, #0086c4 0%, #1796d1 48%, #0083BF 49%, #0083BF 82%, #0096db 100%);
  background-image: -moz-linear-gradient(top, #0086c4 0%, #1796d1 48%, #0083BF 49%, #0083BF 82%, #0096db 100%);
  background-image: -o-linear-gradient(top, #0086c4 0%, #1796d1 48%, #0083BF 49%, #0083BF 82%, #0096db 100%);
  background-image: linear-gradient(top, #0086c4 0%, #1796d1 48%, #0083BF 49%, #0083BF 82%, #0096db 100%);
  border-color: hsl(199, 41%, 37%);
  -webkit-box-shadow: inset 0 0 1px 1px #07b1ff, 0 0 1px 3px rgba(0, 0, 0, 0.15);
  -moz-box-shadow: inset 0 0 1px 1px #07b1ff, 0 0 1px 3px rgba(0, 0, 0, 0.15);
  box-shadow: inset 0 0 1px 1px #07b1ff, 0 0 1px 3px rgba(0, 0, 0, 0.15);
  -webkit-text-shadow: 1px 1px 1px #003d59;
  -moz-text-shadow: 1px 1px 1px #003d59;
  -o-text-shadow: 1px 1px 1px #003d59;
  text-shadow: 1px 1px 1px #003d59;
  -webkit-border-radius: 11px;
  -moz-border-radius: 11px;
  border-radius: 11px;
  padding: 3.5999999999999996px 9px 3.5999999999999996px 9px;
}

