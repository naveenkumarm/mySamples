
/*************************************/
body, html, *, #shell	{ margin:0; padding:0; }
body, html				{ background-color:#ffffff; font-size:14px; font-family:arial; }
a img					{ border:none; }
a						{cursor :pointer;}
/*MAIN LAYOUT ELEMENT*/
#shell		{ 
	margin:0 auto; 
	width:100%; 
	position:relative;  
	}

/*Header*/
#header	{ 
	width:100%;
	height:75px; 
	background-color:#ffffff;
  	background-repeat: repeat-x;
	border-bottom:2px #dbe4f5 solid;
	float:left; 
}
.logo	{ 
	width:232px; height:50px; 
	background-image:url(/ems-web/images/logo.png); 
	background-repeat:no-repeat; 
	text-indent:-999em; 
	display:block; 
	float:left; 
	margin:13px 0 0 13px; 
	}

/*Leftpanel*/
#left-panel { 
	width:20%; 
	background-color:#dbe4f5; 
	height:500px; 
	float:left; 
}

/*content*/
#content	{ 
	width:80%; 
	height:500px;
	float:left;
	overflow-y: auto; 
	}
	
/*Footer*/
#footer	{ width:100%;
		height:40px;
		background-color:#ffffff;
		border-top:2px #dbe4f5 solid;
		float:left;
		}

	
.grid {   background-color: white; border: 1px solid #CCCCCC; margin-left: 3%; margin-right: 3%;  margin-top: 10px; width: 94%; }
.grid input { font-family: Arial; }
.grid b { font-weight: bold; }
.grid p { margin-top: 0.9em; margin-bottom: 0.9em; }
.grid select[multiple] { width: 100%; height: 8em; }
.grid h2 { margin-top: 0.4em; font-weight: bold; font-size: 1.2em; }

.grid th {  font-size: 13px;  font-weight: normal;   text-align: center; }
.grid thead {background-color: grey;    border-radius: 7px 7px 0 0;color: #ffffff;}
.grid tbody {background-color: #F0F0F0;}
.grid table, .grid td {border-width: 0; margin: 0; vertical-align: top; font-size:13px;}
.grid th {border-width: 0; margin: 0; vertical-align: middle; font-size:13px; padding:5px 0; }
.grid td input, .grid td select { width: 5em; }
.grid td.remarks input { width: 3em; }
.grid td select { height: 1.5em; white-space: nowrap; width: 6em;}
#projectInfoGrid tbody tr:hover {
    background-color: #E5E5E5 !important;
    color: black;
    cursor: pointer;
}
/*CLEAR FIX*/
.clearfix:after {	content: "."; display: block;	clear: both; visibility: hidden; line-height: 0; height: 0; } 
.clearfix 		{	display: inline-block; }
html[xmlns] .clearfix {	display: block; }
* html .clearfix { height: 1%; }


